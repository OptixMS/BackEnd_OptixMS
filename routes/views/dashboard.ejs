<h2>Prediksi Berdasarkan Tanggal</h2>

<% if (messages.error) { %>
  <div style="color: red;"><%= messages.error[0] %></div>
<% } %>

<form action="/<%= username %>/dashboard/predictpage" method="POST" enctype="multipart/form-data">
  <label for="tanggal">Tanggal:</label><br>
  <input type="date" name="tanggal" required><br><br>

  <label for="dummy_file">Upload File CSV Dummy:</label><br>
  <input type="file" name="dummy_file" accept=".csv" required><br><br>

  <button type="submit">Prediksi</button>
</form>